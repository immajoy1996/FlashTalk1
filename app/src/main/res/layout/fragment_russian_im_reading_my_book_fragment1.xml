<?xml version="1.0" encoding="utf-8"?>

<android.support.v4.widget.NestedScrollView xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@drawable/river1"
    tools:context="com.languageapp.immanuel.flashtalk.RussianImReadingMyBookFragment1">



    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        tools:context="com.languageapp.immanuel.flashtalk.RussianImReadingMyBookFragment1">


        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/toolbar_size"
            android:layout_alignParentTop="true"
            android:background="@color/medium_sea_green"
            app:layout_constraintTop_toTopOf="parent"
            app:titleTextColor="@color/white_smoke">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_gravity="center"
                >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center">

                    <ImageButton
                        android:id="@+id/back_button"
                        android:layout_width="@dimen/arrow_button_width"
                        android:layout_height="@dimen/arrow_button_height"
                        android:background="@drawable/arrow_back_pressed" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="@integer/imreadingmybook_fragment1_title_weight"
                    android:gravity="center|left"
                    android:text="I'm Reading"
                    android:textColor="@color/white"
                    android:textSize="@dimen/toolbar_font_size"
                    android:fontFamily="@font/treb_ms_regular"

                    />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:visibility="gone"
                    android:gravity="center">

                    <com.languageapp.immanuel.flashtalk.CircularImageView2
                        android:id="@+id/audio_white"
                        android:layout_width="@dimen/circular_button_volume_width"
                        android:layout_height="@dimen/circular_button_volume_height"
                        android:src="@drawable/volumetrans2"
                        android:visibility="visible"
                        />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center">

                    <ImageButton
                        android:id="@+id/forward_button"
                        android:layout_width="@dimen/arrow_button_width"
                        android:layout_height="@dimen/arrow_button_height"
                        android:background="@drawable/arrow_forward_pressed" />

                </LinearLayout>

            </LinearLayout>

        </android.support.v7.widget.Toolbar>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/imreadingmybook_fragment1_padding_intro"
            android:id="@+id/linearlayout1"
            app:layout_constraintTop_toBottomOf="@+id/toolbar"
            android:layout_marginTop="@dimen/imreadingmybook_fragment1_top_spacing"
            >

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >

                    <TextView
                        android:id="@+id/intro"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:fontFamily="@font/treb_ms_regular"
                        android:gravity="center"
                        android:padding="@dimen/imreadingmybook_fragment1_padding_intro"
                        android:text="читать is a regular verb."
                        android:textColor="@color/black"
                        android:textSize="@dimen/imreadingmybook_fragment1_font_size_intro" />


                </android.support.v7.widget.CardView>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearlayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/imreadingmybook_fragment1_vertical_spacing"
            android:orientation="vertical"
            android:padding="@dimen/imreadingmybook_fragment1_padding_intro"
            app:layout_constraintTop_toBottomOf="@+id/linearlayout1"
            tools:layout_editor_absoluteX="0dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >

                    <LinearLayout
                        android:id="@+id/linearLayout1"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:background="@drawable/rect_medium_sea_green"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="читать"
                            android:textColor="@color/white"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="cheet-at"
                            android:textColor="@color/white"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_translit"
                            android:textStyle="italic" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="to read"
                            android:textColor="@color/white"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLayout2"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:background="@drawable/rect_white_smoke"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="я читаю"
                            android:textColor="@color/black"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="cheet-a-yu"
                            android:textColor="@color/transliteration_color"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_translit"
                            android:textStyle="italic" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="I read"
                            android:textColor="@color/black"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLayout3"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:background="@drawable/rect_white_smoke"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="ты читаешь"
                            android:textColor="@color/black"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="cheet-a-yu"
                            android:textColor="@color/transliteration_color"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_translit"
                            android:textStyle="italic" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="you read"
                            android:textColor="@color/black"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLayout4"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:background="@drawable/rect_white_smoke"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="он/она читает"
                            android:textColor="@color/black"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="cheet-a-yet"
                            android:textColor="@color/transliteration_color"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_translit"
                            android:textStyle="italic" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="he/she reads"
                            android:textColor="@color/black"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLayout5"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:background="@drawable/rect_white_smoke"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="мы читаем"
                            android:textColor="@color/black"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="cheet-a-yem"
                            android:textColor="@color/transliteration_color"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_translit"
                            android:textStyle="italic" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="we read"
                            android:textColor="@color/black"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLayout6"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:background="@drawable/rect_white_smoke"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="вы читаете"
                            android:textColor="@color/black"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="cheet-a-yet-ye"
                            android:textColor="@color/transliteration_color"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_translit"
                            android:textStyle="italic" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="you(pl) read"
                            android:textColor="@color/black"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLayout7"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:background="@drawable/rect_white_smoke"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="они читают"
                            android:textColor="@color/black"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="cheet-a-yut"
                            android:textColor="@color/transliteration_color"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_translit"
                            android:textStyle="italic" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/tothink_fragment1_height"
                            android:layout_weight="1"
                            android:fontFamily="@font/treb_ms_regular"
                            android:gravity="center"
                            android:padding="@dimen/imreadingmybook_fragment1_padding_boxes"
                            android:text="they read"
                            android:textColor="@color/black"
                            android:textSize="@dimen/imreadingmybook_fragment1_font_size_word" />


                    </LinearLayout>

                </LinearLayout>


        </LinearLayout>


    </android.support.constraint.ConstraintLayout>

</android.support.v4.widget.NestedScrollView>
